{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key",
    "PINECONE_API_KEY": "@pinecone_api_key"
  },
  "functions": {
    "main.py": {
      "maxDuration": 60
    }
  }
} 